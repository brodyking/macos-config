{
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Catppuccin Frapp√©",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.minimap.enabled": false,
    "vim.statusBarColorControl": false,
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideLineNumbers": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBorder": "#242634",
        "editor.lineHighlightBackground": "#2A2C3C",
    },
    "debug.enableStatusBarColor": false,
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.fontFamily": "Ubuntu Mono",
    "editor.fontSize": 14,
    "editor.lineNumbers": "relative",
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.enabled": false,
    "window.density.editorTabHeight": "compact",
    "workbench.editor.showIcons": false,
    "workbench.navigationControl.enabled": false,
    "chat.commandCenter.enabled": false,
    "window.commandCenter": false,
    "workbench.activityBar.location": "top",
    "vim.useSystemClipboard": true,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
    "markdown-preview-enhanced.revealjsTheme": "black.css",
    "VSCodeCounter.exclude": [
        "**/.gitignore",
        "**/.vscode/**",
        "**/node_modules/**",
        "**/assets/**"
    ],
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.layoutControl.enabled": false,
    "workbench.editorAssociations": {
        "*.doc": "default"
    },
    "vim.overrideCopy": false,
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false,
        "<C-w>": false,
        "<C-p>": false,
        "<C-f>": false
    },
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "terminal.integrated.fontFamily": "Ubuntu Mono",
    "terminal.integrated.profiles.windows": {

        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    }
}